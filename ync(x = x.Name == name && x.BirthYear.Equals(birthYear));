[32m+[m[32mÔªøusing MayTheFourth.Core.Entities;[m
[32m+[m[32musing MayTheFourth.Core.Interfaces.Repositories;[m
[32m+[m[32musing MediatR;[m
[32m+[m
[32m+[m[32mnamespace MayTheFourth.Core.Contexts.PersonContext.UseCases.Create;[m
[32m+[m
[32m+[m[32mpublic class Handler : IRequestHandler<Request, Response>[m
[32m+[m[32m{[m
[32m+[m[32m    private readonly IPersonRepository _personRepository;[m
[32m+[m[32m    public Handler(IPersonRepository personRepository)[m
[32m+[m[32m    {[m
[32m+[m[32m        _personRepository = personRepository;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public async Task<Response> Handle(Request request, CancellationToken cancellationToken)[m
[32m+[m[32m    {[m
[32m+[m[32m        #region Validation[m
[32m+[m[32m        // Need to implement a validation system[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        #region Check if person already exists[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            var exists = await _personRepository.AnyAsync(request.Name, request.BirthYear);[m
[32m+[m[32m            if (exists)[m
[32m+[m[32m                return new Response("Erro: Personagem j√° cadastrado.", 400);[m
[32m+[m[32m        }[m
[32m+[m[32m        catch (Exception ex)[m
[32m+[m[32m        {[m
[32m+[m[32m            return new Response($"Erro: {ex.Message}", 500);[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        #region Create a new person[m
[32m+[m[32m        Person? person;[m
[32m+[m[32m        try[m
[32m+[m[32m        {[m
[32m+[m[32m            person = CreatePerson(request);[m
[32m+[m[32m            await _personRepository.SaveAsync(person, cancellationToken);[m
[32m+[m[32m        }[m
[32m+[m[32m        catch(Exception ex)[m
[32m+[m[32m        {[m
[32m+[m[32m            return new Response($"Erro: {ex.Message}", 500);[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m[32m        #region Response[m
[32m+[m[32m        return new Response("Personagem cadastrado com sucesso.", new ResponseData(person));[m
[32m+[m[32m        #endregion[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private Person CreatePerson(Request request)[m
[32m+[m[32m    {[m
[32m+[m[32m        var person = new Person[m
[32m+[m[32m        {[m
[32m+[m[32m            Name = request.Name,[m
[32m+[m[32m            Height = request.Height,[m
[32m+[m[32m            Mass = request.Mass,[m
[32m+[m[32m            HairColor = request.HairColor,[m
[32m+[m[32m            SkinColor = request.SkinColor,[m
[32m+[m[32m            EyeColor = request.EyeColor,[m
[32m+[m[32m            BirthYear = request.BirthYear,[m
[32m+[m[32m            Gender = request.Gender,[m
[32m+[m[32m            Homeworld = request.Homeworld,[m
[32m+[m[32m            Films = request.Films,[m
[32m+[m[32m            Species = request.Species,[m
[32m+[m[32m            Vehicles = request.Vehicles,[m
[32m+[m[32m            Starships = request.Starships,[m
[32m+[m[32m            Created = request.Created,[m
[32m+[m[32m            Edited = request.Edited,[m
[32m+[m[32m            Url = request.Url[m[41m            [m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        return person;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
